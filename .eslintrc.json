{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": "warn", 
        "no-use-before-define": "warn", 
        "spaced-comment": "warn",
        "react/button-has-type": "off",
        "camelcase": "off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]

    }
}
